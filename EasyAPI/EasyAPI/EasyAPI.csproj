<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>The Controller API is automatically generated by convention</Description>
    <SignAssembly>false</SignAssembly>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/amerina/EasyAPI</RepositoryUrl>
    <PackageProjectUrl>https://github.com/amerina/EasyAPI</PackageProjectUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>ASP.NET Core,Dynamic WebApi</PackageTags>
    <Authors>Amerina</Authors>
    <PackageReleaseNotes>The Controller API is automatically generated by convention</PackageReleaseNotes>
    <Product>EasyAPICore</Product>
    <PackageId>EasyAPICore</PackageId>
    <Copyright>Amerina</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
