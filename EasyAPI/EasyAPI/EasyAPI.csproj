<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>The Controller API is automatically generated by convention</Description>
    <SignAssembly>false</SignAssembly>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/amerina/EasyAPICore</RepositoryUrl>
    <PackageProjectUrl>https://github.com/amerina/EasyAPICore</PackageProjectUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>ASP.NET Core,Dynamic WebApi</PackageTags>
    <Authors>Amerina</Authors>
    <PackageReleaseNotes>The Controller API is automatically generated by convention</PackageReleaseNotes>
    <Product>EasyAPICore</Product>
    <PackageId>EasyAPICore</PackageId>
    <Copyright>Amerina</Copyright>
    <AssemblyName>EasyAPICore</AssemblyName>
    <RootNamespace>EasyAPICore</RootNamespace>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
</Project>
